RewriteEngine on
RewriteBase /

#Redirecionamento de Buscas

RewriteCond %{QUERY_STRING} !=""
RewriteCond %{QUERY_STRING} ^busca=([0-9-a-z\-+]+)$ [NC]
RewriteRule ^(.*)$ /CandiDados/pesquisa/%1? [R]
RewriteRule ^pesquisa/([^/]*)$ /CandiDados/pesquisa.php?query=$1
RewriteRule ^pesquisa/([^/]*)/([^/]*)$ /CandiDados/pesquisa.php?query=$1&pg=$2

#Redirecionamento para Ano Eleitoral
RewriteRule ^eleicoes-([^/]*)$ /CandiDados/conteudo.php?ano=$1

#Redirecionamento para Candidatos
RewriteRule ^eleicoes-([^/]*)/candidatos/([^/]*)$ /CandiDados/conteudo.php?ano=$1&candidato=$2

#Redirecionamentos de Estados
RewriteRule ^eleicoes-([^/]*)/acre$ /CandiDados/conteudo.php?ano=$1&estado=AC
RewriteRule ^eleicoes-([^/]*)/amapa$ /CandiDados/conteudo.php?ano=$1&estado=AP
RewriteRule ^eleicoes-([^/]*)/amazonas$ /CandiDados/conteudo.php?ano=$1&estado=AM
